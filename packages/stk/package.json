{
	"name": "stk",
	"version": "1.0.0",
	"description": "Shared tookkit for web apps.",
	"author": "Whatreal",
	"license": "MIT",
	"homepage": "https://github.com/openages/animus/packages/stk",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/openages/animus.git"
	},
	"sideEffects": false,
	"files": ["./dist", "./package.json", "./README.md"],
	"exports": {
		"./common": {
			"types": "./dist/common/index.d.ts",
			"require": "./dist/common/index.js",
			"import": "./dist/common/index.js",
			"default": "./dist/common/index.js"
		},
		"./dnd": {
			"types": "./dist/dnd/index.d.ts",
			"import": "./dist/dnd/index.js",
			"default": "./dist/dnd/index.js"
		},
		"./emittery": {
			"types": "./dist/emittery/index.d.ts",
			"import": "./dist/emittery/index.js",
			"default": "./dist/emittery/index.js"
		},
		"./mobx": {
			"types": "./dist/mobx/index.d.ts",
			"import": "./dist/mobx/index.js",
			"default": "./dist/mobx/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js",
			"default": "./dist/react/index.js"
		},
		"./storage": {
			"types": "./dist/storage/index.d.ts",
			"import": "./dist/storage/index.js",
			"default": "./dist/storage/index.js"
		},
		"./creep": {
			"types": "./dist/creep/index.d.ts",
			"import": "./dist/creep/index.js",
			"default": "./dist/creep/index.js"
		},
		"./dom": {
			"types": "./dist/dom/index.d.ts",
			"import": "./dist/dom/index.js",
			"default": "./dist/dom/index.js"
		},
		"./graph": {
			"types": "./dist/graph/index.d.ts",
			"import": "./dist/graph/index.js",
			"default": "./dist/graph/index.js"
		},
		"./typekit": {
			"types": "./dist/typekit/index.d.ts",
			"import": "./dist/typekit/index.js",
			"default": "./dist/typekit/index.js"
		}
	},
	"scripts": {
		"dev": "wait bun...",
		"build": "bun bun.config.ts && bun ./node_modules/typescript/lib/tsc.js",
		"build:test": "rspack build"
	},
	"keywords": ["react", "nodejs", "web", "utils", "typescript"],
	"dependencies": {
		"@swc/helpers": "^0.5.17",
		"ahooks": "^3.8.4",
		"fast-equals": "^5.2.2",
		"lodash-es": "^4.17.21",
		"mobx": "^6.13.7",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"scheduler": "^0.26.0"
	},
	"devDependencies": {
		"@rspack/cli": "1.3.5",
		"@rspack/core": "1.3.5",
		"@types/fs-extra": "^11.0.4",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.14.1",
		"@types/react": "19.1.2",
		"@types/react-dom": "^19.1.2",
		"@types/scheduler": "^0.26.0",
		"bun": "^1.2.10",
		"bun-types": "^1.2.10",
		"fast-glob": "^3.3.3",
		"fs-extra": "^11.3.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
