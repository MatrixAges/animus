		"./llm": {
			"types": "./dist/llm/index.d.ts",
			"import": "./dist/llm/index.js",
			"default": "./dist/llm/index.js"
		},
		"./prompt": {
			"types": "./dist/prompt/index.d.ts",
			"import": "./dist/prompt/index.js",
			"default": "./dist/prompt/index.js"
		}
	},
	"dependencies": {
		"atomically": "^2.0.3",
		"await-to-js": "^3.0.0",
		"classic-level": "^3.0.0",
		"dayjs": "^1.11.13",
		"deepmerge-ts": "^7.1.5",
		"mnemonist": "^0.40.3",
		"ofetch": "^1.4.1",
		"zod": "^4.0.5",
		"stk": "workspace:*"
	},
	"devDependencies": {
		"@rslib/core": "catalog:",
		"@types/node": "^24.0.14",
		"bun": "^1.2.18",
		"bun-types": "^1.2.18",
		"concurrently": "^9.2.0",
		"cross-env": "^7.0.3",
		"typescript": "^5.8.3"
